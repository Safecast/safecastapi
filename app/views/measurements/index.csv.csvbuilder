csv << [
  'Captured Time',
  'Latitude',
  'Longitude',
  'Value',
  'Unit',
  'Location Name',
  'Device ID',
  'MD5Sum',
  'Height',
  'Surface',
  'Radiation',
  'Uploaded Time',
  'Loader ID'
]
@result.each do |measurement|
  csv << [
    measurement.captured_at,
    measurement.latitude,
    measurement.longitude,
    measurement.value,
    measurement.unit,
    measurement.location_name,
    measurement.device_id,
    measurement.md5sum,
    measurement.height,
    measurement.surface,
    measurement.radiation,
    measurement.created_at,
    measurement.measurement_import_id
  ] 
end