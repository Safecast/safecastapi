    <%- @bgeigie_logs.each do |bgeigie_log| -%>
      <Placemark>
        <name><%= bgeigie_log.usv %></name>
        <description>CPM Value = <%= bgeigie_log.cpm %>
<%= bgeigie_log.captured_at %></description>
        <styleUrl>#<%= bgeigie_log.icon_color %></styleUrl>
        <Point>
          <altitudeMode>clampToGround</altitudeMode>
          <coordinates><%= bgeigie_log.coordinates %></coordinates>
        </Point>
      </Placemark>
